package Gestore_spese;

import java.util.Scanner;
import java.time.LocalDateTime;
@Getter
@Setter

public class Spese{

    private String nome;
    private double valore;
    private String valuta;
    private String tipologia;
    private LocalDateTime data;
    

    public Spese(String nome, double valore, String valuta, String tipologia, LocalDate data)
    {
        this.nome=nome;
        this.valore=valore;
        this.valuta=valuta;
        this.tipologia=tipologia;
        this.data=data;
    }




}